package com.runner.planets;

public class PlanetLvl extends BasePlanet{

	public void render(){
		switch(this.gameS){
		case 1:
			this.mainGame();
			break;
		case 2:
			this.listener.setPts(pts);
			this.finished();
			break;
		case 3: 
			this.gameOver();
			break;
		case 4:
			this.pauseGame();
			break;
		}
	}
}
