package game.world;

public class Obstacle {
	public static final int TYPE_1 = 0;
	public static final int TYPE_2 = 1;
	public static final int TYPE_3 = 2;
	public static final int TYPE_4 = 3;
	
	public int x,y,tipo;
	
	public Obstacle(int x, int y, int tipo){
		this.x = x;
		this.y = y;
		this.tipo = tipo;
	}
}
